.align_middle{
	display: inline-block;
	vertical-align: middle;
}
@font-face {
  font-family: "San Francisco";
  font-weight: 100;
  src: url("fonts/sanfranciscodisplay-ultralight-webfont.woff");
}

/** Thin */
@font-face {
  font-family: "San Francisco";
  font-weight: 200;
  src: url("fonts/sanfranciscodisplay-thin-webfont.woff");
}

/** Regular */
@font-face {
  font-family: "San Francisco";
  font-weight: 400;
  src: url("fonts/sanfranciscodisplay-regular-webfont.woff");
}

/** Medium */
@font-face {
  font-family: "San Francisco";
  font-weight: 500;
  src: url("fonts/sanfranciscodisplay-medium-webfont.woff");
}

/** Semi Bold */
@font-face {
  font-family: "San Francisco";
  font-weight: 600;
  src: url("fonts/sanfranciscodisplay-semibold-webfont.woff");
}

/** Bold */ 
@font-face {
  font-family: "San Francisco";
  font-weight: 700;
  src: url("fonts/sanfranciscodisplay-bold-webfont.woff");
}
html,body{
	width:100%;
	margin: 0;
	height:100%;
	font-family: Arial;

 -webkit-tap-highlight-color: transparent;
}
a{
color:orange;
}
h3{color:white;}
.ui-button:hover{
font-weight:200;
}
.ui-widget{
font-family: "San Francisco";
font-weight:200 !important;
}
.ui-button{
background-color:white;
font-family: San Francisco;
	font-weight:200;padding:1%; font-size: 130%;
border:0;
}
.ui-state-active,
.ui-widget-content .ui-state-active,
.ui-widget-header .ui-state-active,
a.ui-button:active,
.ui-button:active,
.ui-button.ui-state-active:hover {
	border: 1px solid <%= global_color %>;
	background: <%= global_color %>;
	font-weight: normal;
	color: #ffffff;
} 
#createNewRecipe{
display:none;
-moz-appearance:none;-webkit-appearance:none;-appearance:none;

border-radius:0;border:0;
	text-align:center;
	box-shadow: 0 2px 2px 2px orange;
	background-color: white;
color:orange;
	cursor:pointer;

	font-family: San Francisco;
	font-weight:200;padding:1%; font-size: 100%;
}
#modifyRecipe {
	cursor:pointer;

display:none;
-moz-appearance:none;-webkit-appearance:none;-appearance:none;

border-radius:0;border:0;
	text-align:center;
	box-shadow: 0 2px 2px 2px lightgreen;
	background-color: white;
color: lightgreen;
	font-family: San Francisco;
	font-weight:200;padding:1%; font-size: 100%;
}
#saveConditions{
display:none;
-moz-appearance:none;-webkit-appearance:none;-appearance:none;
	cursor:pointer;

border-radius:0;border:0;
	text-align:center;
	box-shadow: 0 2px 2px 2px <%= global_color %>;
	background-color: white;
color: <%= global_color %>;
width:30%;
	font-family: San Francisco;
	font-weight:200;padding:1%; font-size: 100%;
margin-bottom:3%;
}
#deleteConditions{
text-align:center;
display:none;
}
#deleteConditionsButt{
	cursor:pointer;

-moz-appearance:none;-webkit-appearance:none;-appearance:none;
margin:0px auto 30px auto;

border-radius:0;border:0;
	text-align:center;
	box-shadow: 0 2px 2px 2px red;
	background-color: white;
color: red;
width:30%;
	font-family: San Francisco;
	font-weight:200;padding:1%; font-size: 100%;

margin-bottom:3%;
}
#blocklyDiv{

display:none;
margin:2% auto;
height:800px;
}
#saveConditionsFile{-moz-appearance:none;-webkit-appearance:none;-appearance:none;
	cursor:pointer;

border-radius:0;border:1px  #D8D8D8 solid;
	text-align:center;
	width:60%;
	display:none;
	font-family: San Francisco;
	font-weight:200;padding:1%; font-size: 100%;
margin-bottom:3%;
}
#conditionsButtons,#buttonsTopRecipe{
display:flex;
	cursor:pointer;

	justify-content: space-between;
	align-items: center;


}
.updateRecipeSelect{
	cursor:pointer;

	border: 1px solid <%= global_color %>;
	background-color: white;
color: <%= global_color %>;
border-radius:15px;
text-align:center;
margin:4% auto 0% auto;
padding:1%;
}
#cp9_container,#cp8_container{
text-align:center;
}
#styleBox{
text-align:center;
}
#flexStyle{
display:flex;
flex-wrap: wrap;
justify-content: space-around;
}
#saveColors{
margin-left:50%;
}
#resetColors,#saveColors{background: 0;
			border: 1px solid #D8D8D8;
text-align: center;
display: inline-block;
	color: #D8D8D8;
	padding: 1%;
	font-size: 90%;
border-radius: 15px;
cursor:pointer;
margin: 5% 5% ;}
.ui-progressbar {
    background-color: #ccc;
width:80%
margin:2% auto;
  }
  #progressbar .ui-progressbar-value,.ui-progressbar-indeterminate{
width:100%;
}
#styleState{
color:white;
margin-top:3%;
}
#showConditionCode{
    background-color: white;
	cursor:pointer;
    color: <%= global_color %>;
	border:1px solid <%= global_color %>;
    display: table;
    padding: 1%;
    border-radius: 50px;
    text-align: center;
    margin: 1% auto;
    font-size: 90%;
	}
	#genratorBlockly{
	display:none;
	background-color:white;
	border-radius:50px;
	color: black;
    font-size: 90%;
    border: 2px solid yellow;
    letter-spacing: 0.7px;
padding:3% 2%;
        font-family: monospace;
	white-space: pre-wrap;
	}
	#showConditionCode{
	display:none;
	}
	
